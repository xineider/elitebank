	<%if(usuario.nivel == 1){%>

	<div class="modal-header primary">
		<h5 class="modal-title">Mensagem Usuários</h5>
		<button type="button" class="close pointer" data-dismiss="modal" aria-label="Fechar">
			<span aria-hidden="true">&times;</span>
		</button>
	</div>

	<div class="padding-10">
		<div class="block-body margin-t-10">
			<% if (typeof data.mensagem_a != 'undefined' && data.mensagem_a.length > 0) {
				for (var i = 0; i < data.mensagem_a.length; i++) {%>


				<div class="alert <%=
				data.mensagem_a[i].tipo_mensagem == 1 ? 'sucess-block' : data.mensagem_a[i].tipo_mensagem == 2 ? 'warning-block' : 'error-block' %> alert-dismissible fade show" role="alert">
				<%= data.mensagem_horario[i]%><%= data.mensagem_a[i].descricao_mensagem %>
			</div>
			<%}
		}else{%>
		<div class="text-center">Não há nenhuma mensagem no momento.</div>

		<%}%>
	</div>

</div>

<%}else{%>
<div class="padding=10">
	Você não pode acessar essa funcionalidade!!!
</div>
<%}%>